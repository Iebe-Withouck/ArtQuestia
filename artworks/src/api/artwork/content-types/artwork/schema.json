{
  "kind": "collectionType",
  "collectionName": "artworks",
  "info": {
    "singularName": "artwork",
    "pluralName": "artworks",
    "displayName": "Artworks"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Creator": {
      "type": "string"
    },
    "Year": {
      "type": "biginteger"
    },
    "Theme": {
      "type": "string"
    },
    "Description": {
      "type": "text"
    },
    "Photo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Photo_Hidden": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Location": {
      "type": "customField",
      "customField": "plugin::geodata.geojson",
      "options": {
        "info": true
      }
    },
    "Stickers": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Stickers_Hidden": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Color": {
      "type": "string"
    },
    "themes": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::theme.theme",
      "inversedBy": "artworks"
    }
  }
}
